{% extends 'global/layout_form.html.twig' %}

{% set titlePage = 'page.empleados.title.new' | trans %}
{% set menu = "empleados" %}
{% set pathCancel = path('empleados_list') %}

{% block formContent %}
<div class="znv-half">
    {{ form_row(form.name) }}
</div>
<div class="znv-half">
  {{ form_row(form.state) }}
</div>
<div class="znv-half">
  {{ form_row(form.city) }}
</div>
<div class="znv-half">
  {{ form_row(form.address) }}
</div>
<div class="znv-half">
  {{ form_row(form.phone) }}
</div>
<div class="znv-half">
  {{ form_row(form.email) }}
</div>
<div class="znv-half">
  {{ form_row(form.status) }}
</div>
{% endblock %}
